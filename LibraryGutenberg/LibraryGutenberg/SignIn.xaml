<Window x:Class="LibraryGutenberg.SignIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryGutenberg"
        mc:Ignorable="d"
        Title="SignIn" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBox x:Name="tboxLogin"></TextBox>
        <TextBox x:Name="tboxPassword"></TextBox>
        <Button x:Name="signInBtn" Content="Войти" Click="signInBtn_Click"></Button>
    </StackPanel>
</Window>
