<Window x:Class="LibraryGutenberg.SignUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryGutenberg"
        mc:Ignorable="d"
        Title="SignUp" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBox x:Name="tboxName"></TextBox>
        <TextBox x:Name="tboxLogin"></TextBox>
        <TextBox x:Name="tboxPassword"></TextBox>
        <Button x:Name="SignUpBtn" Content="Зарегистрироваться" Click="SignUpBtn_Click"></Button>
    </StackPanel>
</Window>
